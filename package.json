{
  "name": "bible-plugin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm run dev:web & pnpm run dev:plugin",
    "dev:web": "pnpm --filter web-app dev",
    "dev:plugin": "pnpm --filter plugin dev",
    "build:web": "pnpm --filter web-app build",
    "build:plugin": "pnpm --filter plugin build",
    "build:all": "pnpm -r build",
    "lint": "eslint .",
    "preview:web": "pnpm --filter web-app preview"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.2.5"
    }
  }
}
